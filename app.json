{
    "name": "VAJIRA-MD",
    "version":"4.0.0",
    "description": "Vajira Multi Device whatsappbot",
    "logo": "https://telegra.ph/file/26fa7a93ca90c0edaae45.jpg",
    "keywords": ["whatsapp"],
    "repository": "https://github.com/vajirabot1/VAJIRA-MD",
    "website": "https://github.com/vajirabot1/VAJIRA-MD",
    "success_url": "https://github.com/vajirabot1/VAJIRA-MD",
    "stack": "container",
    "env": {
        "ANTI_LINK": {
            "description": "true/false",
            "required": false
        },
        "ANTILINK_ACTION": {
            "description": "kick",
            "required": false
        },
        "RMBG_KEY": {
            "description": "Remove bg api key ",
            "required": false
            "value": "put bg remove api key"
        },
        "SESSION_ID": {
            "description": "Your Session ID",
            "required": true
        },
        "HANDLERS": {
            "description": "prefix for commands, enter false for no prefix",
            "required": true,
            "value": "."
        },
        "LOGS": {
            "description": "need to show message logs ?",
            "required": false
            "value": "true"
        },
        "PACKNAME": {
            "description": "Sticker Packname",
            "required": false
            "value": "VAJIRA"
        },

        "GOODBYE_MSG": {
            "description": "goodbye Message",
            "required": false
            "value": "false"
        },
        "AUTHOR": {
            "description": "Sticker Pack AUTHOR",
            "required": false
            "value": "VAJIRA"
        },
        "SUDO": {
            "description": "sudo numbers with country code seperate with , for multiple numbers",
            "required": false
        },
        "HEROKU_APP_NAME": {
            "description": "Heroku app name, same as above entered",
            "required": true
        },
        "HEROKU_API_KEY": {
            "description": "Heroku account api key, https://dashboard.heroku.com/account",
            "required": true
        }
    }
}
